@charset "UTF-8";
/* RESET */
html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, embed, figure, figcaption, footer, header, menu, nav, output, ruby, section, summary, time, mark, audio, video {margin: 0;padding: 0;border: 0;font-size: 100%;font: inherit;}
/* HTML5 display-role reset for older browsers */
article, aside, details, figcaption, figure, footer, header, menu, nav, section {display: block;}
body {zoom:1;line-height: 1;font-family:"微软雅黑", Tahoma,Lucida,Verdana,Arial,"宋体",Helvetica,sans-serif;}
ol, ul {list-style: none;}
blockquote, q {quotes: none;}
blockquote:before, blockquote:after, q:before, q:after {content: '';}
table {border-collapse: collapse;border-spacing: 0;}
/* RESET END */
.clear {clear: both;}
.clearfix:after { content: ".";display: block;height: 0;clear: both;visibility: hidden;}
.fl{float: left;}
.fr{float: right;}
.img{border:none;}
.home{width:1100px;height: inherit;margin: auto;position: relative;}
a{text-decoration: none;color: #333;font-size:12px}
.mtb30{margin:30px auto;}
.mt20{margin-top:20px;}
.border-box{border: 1px solid #e2e2e2;}
/*company-logo*/
.logo{
    background: url("../image/public/logo.png") no-repeat;
    width:238px;
    height:50px;
}
/*search block*/
.search{
    margin-left: 120px;
    font-size:12px;
    overflow: hidden;
}
.search input{
    width:300px;
    height:37px;
    line-height:37px;
    font-size:12px;
    outline: none;
    border:1px solid #ea554f;
    padding: 0 5px;
    letter-spacing:1px;
}
#searchBtn{
    display: block;
    background:url("../image/public/search.png") no-repeat;
    width:50px;
    height:39px;
    text-indent: -9999em;
    cursor: pointer;
}
.city{
    margin:8px 15px 0 15px;
    position: relative;
}
.city label{
    color: #00e6c0;
    font-size: 20px;
}
.city span{
    border: 1px solid #e2e2e2;
    padding:3px 10px;
    color: #989898;
    font-size: 12px;
    cursor: pointer;
}
/*globalnav*/
nav{
    position: relative;
    height:50px;
    line-height:50px;
    background:#ea554f;
    z-index:999;
}
nav ul li{
    float:left;
    display: inline;
    border-right: 1px solid #c01c1b;
    position: relative;
    text-align: center;
}
nav ul li:last-child{
    border-right: none;
}
nav ul li:hover,nav ul li.selected {
    transition: all 0.4s;
    background:#00e6c0;
    color:#fff;
}
nav a{
    width: 120px;
    display: inline-block;
    font-size:16px;
    font-weight:normal;
    color:#fff;
    text-decoration: none;
    letter-spacing: 2px;
}
.side-menu {
    width: 218px;
}
.mt10{
    margin-top: 10px;
}
.menu-group-title{
    margin:10px 20px;
}
.icon-item{
    width: 28px;
    height: 24px;
    position: relative;
    top: 5px;
    display: inline-block;
}
.icon-item1{
    background: url("../image/index/icon-item1.png") no-repeat;
}
.icon-item2{
    background: url("../image/index/icon-item2.png") no-repeat;
}
.menu-group-title a{
    font-size: 16px;
}
.menu-items{
    padding: 0 10px;
    overflow: hidden;
}
.menu-items li {
    margin: 0 10px;
    float: left;
    overflow: hidden;
    line-height: 30px;
}
.menu-items li a{
    font-size: 14px;
    display: inline-block;
}
.menu-items li a:hover{
    color:#ea554f;
    text-decoration: underline;
}
.menu-items-info li{
    width:78px;
    text-align: center;
}
.publish-info{
    display: block;
    text-align: center;
    color: #ea554f;
    font-size: 14px;
    padding-bottom:14px;
}
.publish-info .icon-publish{
    background: url("../image/index/icon-publish2.png") no-repeat;
    width: 28px;
    height: 28px;
    display: inline-block;
    position: relative;
    top: 5px;
}
/*侧边栏菜单部分end*/
.login-wrap{
    overflow: hidden;
}

.login-wrap  a{
    display: inline-block;
    float:left;
    margin-left: 25px;
    text-align: center;
    width:50px;
    height:60px;
    font-size: 12px;
}
.login-wrap a .icon{
    margin: auto;
    width: 40px;
    height: 40px;
