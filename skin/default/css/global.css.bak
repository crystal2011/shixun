}
.login-wrap a .icon-contact{
    background: url("../image/public/icon-contact.png") no-repeat;
}
.login-wrap a .icon-collect{
    background: url("../image/public/icon-collect.png") no-repeat;
}
.login-wrap a .icon-publish{
    background: url("../image/public/icon-publish.png") no-repeat;
}
/*footer*/
footer{
    background: #ea554f;
    padding:30px 0 20px;
    color: #fff;
    font-size: 14px;
    text-align: center;
    margin: 20px 0;
}
.about-us a {
    height: 30px;
    line-height: 30px;
    display: inline-block;
    margin: 10px 20px 0 0;
    font-size: 14px;
    color: #fff;
}

.about-us a:hover {
