/*cutline*/
.cut-line{
    line-height: 0;
    font-size: 0;
    overflow: hidden;
    border-top: 1px solid #e6e6e6;
    display: block;
    margin:10px 20px;
}
/*地址栏*/
.sites a {
    display: inline-block;
    height: 35px;
    line-height: 35px;
    font-size: 12px;
    text-decoration: none;
    color: #232323;
    letter-spacing: 1px;
}
/*分页*/
.page-box{
    height: 30px;
    display:table;
    text-align: center;
    margin:25px auto 0;
    *position: relative;/*ie 7 支持*/
}
.page-box .pager{
    vertical-align:middle;
    display:table-cell;
    overflow: hidden;
    *position: absolute;/*ie 7 支持*/
    *top: 0;/*ie 7 支持*/
    *left: 25%;/*ie 7 支持*/
}
.pager li {
    width:40px;
    height:30px;
    text-align: center;
    font-size: 12px;
    line-height:30px;
    border: 1px solid #e2e2e2;
    float: left;
    color: #999;
    margin: 0 3px
}
.pager a {float:left;}
.pager-first-li-disable {
    width: 40px;
    color: #999;
    cursor: not-allowed;
}
.pager-previous-li-disable {
    width: 60px;
    color: #999;
    cursor: not-allowed;
}
.pager-first-li {
    width: 40px;
    cursor: pointer;
}
.pager-previous-li {
    width: 60px;
    cursor: pointer;
}
.pager-first-li:hover ,.pager-previous-li:hover,.pager-normal-li:hover,.pager-next-li:hover,.pager-last-li:hover{
    color:#fff;
    background:#00e6c0;
}
.pager-normal-li{
    cursor: pointer;
}
.pager .pager-index-li {
    background:#00e6c0;
    color: #fff;
    cursor: default;
}
.pager-space-li {
    cursor: not-allowed;
}
.pager-next-li-disable {
    width: 60px;
    color: #999;
    cursor: not-allowed;
}
.pager-last-li-disable {
    width: 40px;
    color: #999;
    cursor: not-allowed;
}
.pager-next-li {
    width: 60px;
    cursor: pointer;
}
.pager-last-li {
    width: 40px;
    cursor: pointer;
}
/*分页end*/
