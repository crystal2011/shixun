    color: #656565;
}
.food-list-box .list-result-item .lists li.selected a,.food-list-box .list-result-item .lists li a:hover{
    color: #ea554f;
    text-decoration: underline;
}
.food-list-box .list-result-item .more i{
    background: url("../image/public/icon-more.png") no-repeat -2px 2px;
    width: 14px;
    height: 14px;
